<mvc:View controllerName="Task4.controller.display" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:uxap="sap.uxap" xmlns="sap.m" xmlns:l="sap.ui.layout">
	<App>
		<uxap:ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false" subSectionLayout="TitleOnLeft" showTitleInHeaderContent="true"
			showHeaderContent="true">
			<uxap:headerTitle>
				<uxap:ObjectPageHeader id="ObjectPageLayoutHeaderTitle"  isObjectTitleAlwaysVisible="false" isActionAreaAlwaysVisible="true">
					<Button class="edit" text="Edit" press=".onEdit"/>
					<Button class="delete" text="Delete"/>
				</uxap:ObjectPageHeader>
			</uxap:headerTitle>
			<uxap:sections>
				<uxap:ObjectPageSection id="section1" title="General Header">
					<uxap:subSections>
						<uxap:ObjectPageSubSection>
							<uxap:blocks>
								<VBox>
									<f:SimpleForm editable="false" id="form" layout="ColumnLayout" columnsM="2" columnsL="3">
										<Label text="Sales Document" labelFor="VBELN"/>
										<Text id="VBELN" text="{myModel>/VBELN}"/>
										<Label text="Created on" labelFor="ERDAT"/>
										<Text id="ERDAT" text="{myModel>/ERDAT}"/>
										<Label text="Proposed date type" labelFor="VPRGR"/>
										<Text id="VPRGR" text="{myModel>/VPRGR}"/>
										<Label text="Created by" labelFor="ERNAM"/>
										<Text id="ERNAM" text="{myModel>/ERNAM}"/>
										<Label text="Sales Doc. Type" labelFor="AUART"/>
										<Text id="AUART" text="{myModel>/AUART}"/>
										<Label text="Net value" labelFor="NETWR"/>
										<Text id="NETWR" text="{myModel>/NETWR}"/>
										<Label text="Division" labelFor="SPART"/>
										<Text id="SPART" text="{myModel>/SPART}"/>
										<Label text="Doc. Currency" labelFor="WAERK"/>
										<Text id="WAERK" text="{myModel>/WAERK}"/>
										<Label text="Sales Org." labelFor="VKORG"/>
										<Text id="VKORG" text="{myModel>/VKORG}"/>
										<Label text="Distr. Channel" labelFor="VTWEG"/>
										<Text id="VTWEG" text="{myModel>/VTWEG}"/>
										<Label text="Warranty Date" labelFor="GWLDT"/>
										<Text id="GWLDT" text="{myModel>/GWLDT}"/>
										<Label text="Customer" labelFor="KUNNR"/>
										<Text id="KUNNR" text="{myModel>/KUNNR}"/>
									</f:SimpleForm>
								</VBox>
							</uxap:blocks>
						</uxap:ObjectPageSubSection>
					</uxap:subSections>
				</uxap:ObjectPageSection>
				<uxap:ObjectPageSection id="section2" title="Sales">
					<uxap:subSections>
						<uxap:ObjectPageSubSection>
							<uxap:blocks>
								<VBox>
									<f:SimpleForm editable="false" id="form1" layout="ColumnLayout" columnsM="2" columnsL="3">
										<Label text="Document Date" labelFor="AUDAT"/>
										<Text id="AUDAT" text="{myModel>/AUDAT}"/>
										<Label text="Sales Org." labelFor="VKORG1"/>
										<Text id="VKORG1" text="{myModel>/VKORG1}"/>
										<Label text="Created by" labelFor="ERNAM1"/>
										<Text id="ERNAM1" text="{myModel>/ERNAM1}"/>
										<Label text="Created on" labelFor="ERDAT1"/>
										<Text id="ERDAT1" text="{myModel>/ERDAT1}"/>
										<Label text="Order reason" labelFor="AUGRU"/>
										<Text id="AUGRU"  text="{myModel>/AUGRU}"/>
										<Label text="Sales Office" labelFor="VKBUR"/>
										<Text id="VKBUR" text="{myModel>/VKBUR}"/>
										<core:Title text="Pricing and Statistics"/>
										<Label text="Doc. Currency" labelFor="WAERK1"/>
										<Text id="WAERK1" text="{myModel>/WAERK1}"/>
										<Label text="Procedure" labelFor="KALSM"/>
										<Text id="KALSM" text="{myModel>/KALSM}"/>
										<Label text="Customer purchase order number " labelFor="BSTNK"/>
										<Text id="BSTNK" text="{myModel>/BSTNK}"/>
										<Label text="Customer purchase order type" labelFor="BSARK"/>
										<Text id="BSARK" text="{myModel>/BSARK}"/>
									</f:SimpleForm>
								</VBox>
							</uxap:blocks>
						</uxap:ObjectPageSubSection>
					</uxap:subSections>
				</uxap:ObjectPageSection>
				<uxap:ObjectPageSection id="section3" title="Shipping">
					<uxap:subSections>
						<uxap:ObjectPageSubSection>
							<uxap:blocks>
								<VBox>
									<f:SimpleForm editable="false" id="form2" layout="ColumnLayout" columnsM="2" columnsL="3">
										<Label text="Complete delivery" labelFor="AUTLF"/>
										<Text id="AUTLF" text="{myModel>/AUTLF}"/>
										<Label text="Delivery block " labelFor="LIFSK"/>
										<Text id="LIFSK" text="{myModel>/LIFSK}"/>
										<Label text="Shipping Cond." labelFor="VSBED"/>
										<Text id="VSBED" text="{myModel>/VSBED}"/>
										<Label text="DG Mgmt Profile" labelFor="PROLI"/>
										<Text id="PROLI" text="{myModel>/PROLI}"/>
									</f:SimpleForm>
								</VBox>
							</uxap:blocks>
						</uxap:ObjectPageSubSection>
					</uxap:subSections>
				</uxap:ObjectPageSection>
				<uxap:ObjectPageSection id="section4" title="Billimg Document">
					<uxap:subSections>
						<uxap:ObjectPageSubSection>
							<uxap:blocks>
								<VBox>
									<f:SimpleForm editable="false" id="form3" layout="ColumnLayout" columnsM="2" columnsL="3">
										<Label text="Company code to be billed " labelFor="BUKRSVF"/>
										<Text id="BUKRSVF" text="{myModel>/BUKRSVF}"/>
										<Label text="EU Triang. Deal" labelFor="XEGDR"/>
										<Text id="XEGDR" text="{myModel>/XEGDR}"/>
										<Label text="Billing block" labelFor="FAKSK"/>
										<Text id="FAKSK" text="{myModel>/FAKSK}"/>
										<Label text="Alt. tax classific." labelFor="TAXK1"/>
										<Text id="TAXK1" text="{myModel>/TAXK1}"/>
										<Label text="Tax departure country" labelFor="LANDTX"/>
										<Text id="LANDTX" text="{myModel>/LANDTX}"/>
									</f:SimpleForm>
								</VBox>
							</uxap:blocks>
						</uxap:ObjectPageSubSection>
					</uxap:subSections>
				</uxap:ObjectPageSection>
				<uxap:ObjectPageSection id="section5" title="Condition">
					<uxap:subSections>
						<uxap:ObjectPageSubSection>
							<uxap:blocks>
								<VBox>
									<f:SimpleForm editable="false" id="form4" layout="ColumnLayout" columnsM="2" columnsL="3">
										<Label text="Net value " labelFor="NETWR1"/>
										<Text id="NETWR1" text="{myModel>/NETWR1}"/>
									</f:SimpleForm>
								</VBox>
							</uxap:blocks>
						</uxap:ObjectPageSubSection>
					</uxap:subSections>
				</uxap:ObjectPageSection>

				<uxap:ObjectPageSection id="section6" title="Items">
					<uxap:subSections>
						<uxap:ObjectPageSubSection>
							<uxap:blocks>
								<VBox>
									<Table class="table" width="97%" id="table" items="{/}" noDataText="{worklistView>/tableNoDataText}" backgroundDesign="Solid">
										<columns>
											<Column id="POSNRCol" hAlign="Center">
												<Text text="Item" id="POSNR"/>
											</Column>
											<Column id="MATNRCol" hAlign="Center">
												<Text text="Material" id="MATNR"/>
											</Column>
											<Column id="MATWACol" hAlign="Center">
												<Text text="Material Entered" id="MATWA"/>
											</Column>
											<Column id="ZMENGCol" hAlign="Center">
												<Text text="Target quantity	" id="ZMENG"/>
											</Column>
											<Column id="KWMENGCol" hAlign="Center">
												<Text text="Order quantity" id="KWMENG"/>
											</Column>
											<Column id="PSTYVKCol" hAlign="Center">
												<Text text="Item category" id="PSTYV"/>
											</Column>
											<Column id="ZIEMECol" hAlign="Center">
												<Text text="Target qty UoM" id="ZIEME"/>
											</Column>
											<Column id="NETWRCol" hAlign="Center">
												<Text text="Net value" id="NETWRI"/>
											</Column>
											<Column id="WERKSCol" hAlign="Center">
												<Text text="Plant" id="WERKS"/>
											</Column>
										</columns>
										<items>
											<ColumnListItem vAlign="Middle">
												<cells>
													<Text class="pressEdit" text="{Posnr}"/>
													<Text class="pressEdit" text="{Matnr}"/>
													<Text class="pressEdit" text="{Matwa}"/>
													<Text class="pressEdit" text="{Zmeng}"/>
													<Text class="pressEdit" text="{Kwmeng}"/>
													<Text class="pressEdit" text="{Pstyv}"/>
													<Text class="pressEdit" text="{Zieme}"/>
													<Text class="pressEdit" text="{Netwr}"/>
													<Text class="pressEdit" text="{Werks}"/>
												</cells>
											</ColumnListItem>
										</items>
										<customData>
											<sap.ui.core:CustomData xmlns:sap.ui.core="sap.ui.core" key="w5g.dt.editable" value="false" id="__data8"/>
										</customData>
									</Table>
								</VBox>
							</uxap:blocks>
						</uxap:ObjectPageSubSection>
					</uxap:subSections>
				</uxap:ObjectPageSection>
			</uxap:sections>
		</uxap:ObjectPageLayout>
	</App>
</mvc:View>