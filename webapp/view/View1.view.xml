						<headerToolbar>
							<OverflowToolbar class="margin-top">
								<content>
									<Button class="sort" icon="sap-icon://sort" press="onOpenSortDialog"/>
									<Button icon="sap-icon://excel-attachment" press="onExport" tooltip="Export to Excel"/>
									<Button icon="sap-icon://filter" press="onOpenFilterDialog"/>
									<Button class="editIcon create " text="Create" tooltip="Create" press=".onCreate"/>
									<Button class="editIcon" id="delete" text="Delete" tooltip="Delete" enabled="false" press="onDelete"/>
								</content>
							</OverflowToolbar>
						</headerToolbar>
						<columns>
							<Column hAlign="Center">
								<CheckBox id="selectAllCheckbox" select="onSelectAll" text=""/>
							</Column>
							<Column id="VBELNCol" hAlign="Center">
								<Text text="Sales Document" id="VBELN"/>
							</Column>
							<Column id="ERDATCol" hAlign="Center">
								<Text text="Created on" id="ERDAT"/>
							</Column>
							<Column id="VPRGRCol" hAlign="Center">
