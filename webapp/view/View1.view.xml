										<core:Item key="Vbeln" text="Sales Document"/>
										<core:Item key="Erdat" text="Created on"/>
										<core:Item key="Vprgr" text="Proposed date type"/>
										<core:Item key="Ernam" text="Created by"/>
										<core:Item key="Auart" text="Sales Doc. Type"/>
										<core:Item key="Netwr" text="Net value"/>
										<core:Item key="Waerk" text="Doc. Currency"/>
										<core:Item key="Vkorg" text="Sales Org."/>
										<core:Item key="Gwldt" text="Warranty Date"/>
										<core:Item key="Kunnr" text="Customer"/>
									</items>
								</Select>
								<RadioButtonGroup id="sortOrderGroup" columns="10">
									<buttons>
										<RadioButton text="Ascending" selected="true"/>
										<RadioButton text="Descending"/>
									</buttons>
								</RadioButtonGroup>
							</HBox>
						</VBox>
